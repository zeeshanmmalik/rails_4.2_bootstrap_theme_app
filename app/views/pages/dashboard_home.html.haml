                .panel
                  .panel-body
                    %span.arrow-alt
                    %span.timeline-icon.purple
                    %span.timeline-date 3:20 pm
                    %h1.purple 29 June | Saturday
                    %p Lorem ipsum dolor sit amet consiquest dio
                    .notification
                      %i.fa.fa-exclamation-sign
                      New task added for
                      %a{:href => "#"} Denial Collins
            %article.timeline-item
              .timeline-desk
                .panel
                  .panel-body
                    %span.arrow
                    %span.timeline-icon.light-green
                    %span.timeline-date 07:49 pm
                    %h1.light-green 10 June | Friday
