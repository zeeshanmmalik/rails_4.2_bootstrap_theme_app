                .panel
                  .panel-body
                    %span.arrow
                    %span.timeline-icon.blue
                    %span.timeline-date 11:35 am
                    %h1.blue 05 July | Monday
                    %p
                      %a{:href => "#"} Anjelina Joli
                      added new album
                      %span
                        %a.blue{:href => "#"} PARTY TIME
                    .album
