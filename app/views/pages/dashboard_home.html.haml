= content_for :page_title, "Dashboard Home - ActiStaff"

= content_for :page_specific_styles do
  / page specific styles go here
  = stylesheet_link_tag "jquery-easy-pie-chart/jquery.easy-pie-chart",
                        "dashboard/owl.carousel"
  
= content_for :page_specific_scripts do
  /page specific javascript goes here
  = javascript_include_tag "dashboard/jquery.sparkline",
                           "jquery-easy-pie-chart/jquery.easy-pie-chart",
                           "dashboard/owl.carousel",
                           "dashboard/jquery.customSelect.min",
                           "dashboard/sparkline-chart",
                           "dashboard/easy-pie-chart",
                           "dashboard/count"


  :javascript
    //owl carousel
      
    $(document).ready(function() {
      $("#owl-demo").owlCarousel({
        navigation : true,
        slideSpeed : 300,
        paginationSpeed : 400,
        singleItem : true,
        autoPlay:true
      });
    });
      
    //custom select box
      
    $(function(){
      $('select.styled').customSelect();
    });


%section.wrapper
  / state overview start
  .row.state-overview
    .col-lg-3.col-sm-6
      %section.panel
        .symbol.terques
          %i.fa.fa-user
        .value
          %h1.count
            0
          %p New Users
    .col-lg-3.col-sm-6
      %section.panel
        .symbol.red
          %i.fa.fa-tags
        .value
          %h1.count2
            0
          %p Sales
    .col-lg-3.col-sm-6
      %section.panel
        .symbol.yellow
          %i.fa.fa-shopping-cart
        .value
          %h1.count3
            0
          %p New Order
    .col-lg-3.col-sm-6
      %section.panel
        .symbol.blue
          %i.fa.fa-bar-chart-o
        .value
          %h1.count4
            0
          %p Total Profit
  / state overview end
  .row
    .col-lg-8
