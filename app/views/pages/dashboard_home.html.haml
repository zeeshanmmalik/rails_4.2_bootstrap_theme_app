          .clearfix  
      / timeline end
    .col-lg-4
      / revenue start
      %section.panel
        .revenue-head
          %span
            %i.fa.fa-bar-chart-o
          %h3 Revenue
          %span.rev-combo.pull-right
            June 2013
        .panel-body
          .row
            .col-lg-6.col-sm-6.text-center
              .easy-pie-chart
                .percentage{"data-percent" => "35"}
                  %span> 35
                  \%
            .col-lg-6.col-sm-6
              .chart-info.chart-position
                %span.increase
                %span Revenue Increase
              .chart-info
                %span.decrease
                %span Revenue Decrease
        .panel-footer.revenue-foot
          %ul
            %li.first.active
              %a{:href => "javascript:;"}
                %i.fa.fa-bullseye
                Graphical
            %li
              %a{:href => "javascript:;"}
                %i.fa.fa-th-large
                Tabular
            %li.last
              %a{:href => "javascript:;"}
