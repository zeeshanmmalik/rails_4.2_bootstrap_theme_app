        .bar
          .title OCT
          .value.tooltips{"data-original-title" => "42%", "data-placement" => "top", "data-toggle" => "tooltip"} 42%
        .bar
          .title NOV
          .value.tooltips{"data-original-title" => "60%", "data-placement" => "top", "data-toggle" => "tooltip"} 60%
        .bar
          .title DEC
          .value.tooltips{"data-original-title" => "90%", "data-placement" => "top", "data-toggle" => "tooltip"} 90%
      / custom chart end
    .col-lg-4
      / new earning start
      .panel.terques-chart
        .panel-body.chart-texture
          .chart
            .heading
              %span Friday
              %strong $ 57,00 | 15%
            .sparkline{"data-data" => "[200,135,667,333,526,996,564,123,890,564,455]", "data-fill-color" => "", "data-height" => "75", "data-highlight-line-color" => "#fff", "data-line-color" => "#fff", "data-line-width" => "1", "data-resize" => "true", "data-spot-color" => "#fff", "data-spot-radius" => "4", "data-type" => "line", "data-width" => "90%"}
        .chart-tittle
          %span.title New Earning
          %span.value
            %a.active{:href => "#"} Market
            |
            %a{:href => "#"} Referal
            |
            %a{:href => "#"} Online
      / new earning end
      / total earning start
      .panel.green-chart
        .panel-body
          .chart
            .heading
              %span June
              %strong 23 Days | 65%
            #barchart
        .chart-tittle
          %span.title Total Earning
          %span.value $, 76,54,678
      / total earning end
  .row
    .col-lg-4
      / user info table start
      %section.panel
        .panel-body
          %a.task-thumb{:href => "#"}
            = image_tag "dashboard/avatar1.jpg"
          .task-thumb-details
            %h1
              %a{:href => "#"} Anjelina Joli
            %p Senior Architect
